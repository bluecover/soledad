image-set($url, $url_2x)
  background-image url($url)
  background-image -webkit-image-set(url($url) 1x, url($url_2x) 2x)
  background-image    -moz-image-set(url($url) 1x, url($url_2x) 2x)
  background-image     -ms-image-set(url($url) 1x, url($url_2x) 2x)
  background-image      -o-image-set(url($url) 1x, url($url_2x) 2x)
