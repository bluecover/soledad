.product-footer-wrapper
  >.title
    text-align: center
    color: $blue_gray_90
    font-weight: bold
    font-size: 20px
    margin-bottom: 40px
  .item
    .cooperation-list
      width: 300px
      li
        display: inline-block
        margin-right: 5px
        background: #fff
        width: 80px
        height: 30px
        text-align: center
      a
        margin: 0
      img
        height: 30px

.m-ins-footer-mobile
  padding-top: 20px
  text-align: center
  padding-bottom: 30px
  >.hd
    color: $blue_gray_90
    font-size: 18px
    font-weight: bold
  >.bd
    li
      font-size: 12px
      display: inline-block
      margin: 15px 4% 0 0
      &:last-child
        margin-right: 0
    a
      color: $gray_60
      &:hover
        color: $gray_80
