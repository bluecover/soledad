$m-partner-footer
  >.hd
    text-align: center
    margin-top: 40px
    margin-bottom: 35px
    @media $mobile
      padding-top: 20px
      margin-top: 0
      margin-bottom: 20px
    .title
      color: $fancy_gray_90
      font-size: 30px
      @media $mobile
        font-size: 16px
  >.bd
    .partners
      font-size: 0
      text-align: center
      margin: 0 auto
      @media $mobile
        width: 300px
      li
        font-size: 14px
        color: $desc_text_color
        display: inline-block
        width: 180px
        height: 54px
        line-height: 54px
        vertical-align: middle
        border: 1px solid $gray_20
        background-color: #fff
        margin: 0 .5% 20px .5%
        @media $mobile
          width: 96px
          height: 34px
          min-width: 0
          line-height: 34px
          margin: 4px 2px
          text-align: center
      .svgicon
        vertical-align: middle
        @media $mobile
          margin: 8px auto 0 auto
          vertical-align: top
      .logo-hz
        width: (25/10)*35px
        height: 25px
        @media $mobile
          width: (16/10)*35 px
          height: 16px
      .logo-yxins
        width: (22/9)*38px
        height: 22px
        @media $mobile
          width: (14/9)*38 px
          height: 14px
      .logo-ms
        height: 25px
        width: (25/39)*184 px
        @media $mobile
          width: (15/39)*184 px
          height: 15px
      .logo-zm
        width: (30/36)*85 px
        height: 30px
        @media $mobile
          width: (18/36)*85 px
          height: 18px
      .logo-700
        width: (30/36)*85 px
        height: 30px
        @media $mobile
          width: (18/36)*85 px
          height: 18px
  .footer-wrapper
    margin: 0
    padding-top: 10px
    padding-bottom: 20px
